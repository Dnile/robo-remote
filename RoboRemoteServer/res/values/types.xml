<resources>
    <!-- all arrays start with an item named "TYPE" to help the code weed out string arrays that we don't care about -->
    <string-array name="String">
        <item>TYPE</item>
        <item>String</item>
    </string-array>
    <string-array name="Integer">
        <item>TYPE</item>
        <item>int</item>
        <item>Integer</item>
        <item>long</item>
        <item>Long</item>
        <item>Float</item>
        <item>float</item>
    </string-array>
    <string-array name="Boolean">
        <item>TYPE</item>
        <item>boolean</item>
        <item>Boolean</item>
    </string-array>
</resources>